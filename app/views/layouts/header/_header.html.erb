<header>
  <% if logged_in? %>
  <ul id="dropdown1" class="dropdown-content">
    <li> <a href="<%= user_path current_user %>">profile</a></li>
    <li><a href="<%= edit_user_path current_user %>">Settings</a></li>
    <li class="divider"></li>
    <li><%= link_to "Logout", logout_path, method: :delete %></li>
  </ul>
  <% end %>
  <nav class="light-blue lighten-1" role="navigation">
    <div class="nav-wrapper container"><a id="logo-container" href="<%= root_path %>" class="brand-logo">Fasbagram</a>
      <ul class="right">
        <% if logged_in? %>
          <li><a class="dropdown-button" href="#" data-activates="dropdown1"><%= current_user.user_name %></a></li>
        <% else %>
          <li><a class="dropdown-button" href="<%= new_user_path %>" >register</a></li>
          <li><a class="dropdown-button" href="<%= login_path %>" >login</a></li>
        <% end %>
      </ul>
    </div>
  </nav>
</header>